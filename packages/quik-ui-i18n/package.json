{
  "name": "quik-ui-i18n",
  "private": false,
  "version": "0.0.1",
  "description": "QuikUI i18n Library",
  "main": "lib/index.js",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "scripts": {
    "build": "tsc && npm run api:extract && npm run api:document",
    "watch": "tsc --watch",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown --input-folder api/temp --output-folder api/docs"
  },
