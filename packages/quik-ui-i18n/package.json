  },
  "bugs": {
    "url": "https://github.com/quik-ui/quik-ui/issues"
  },
  "homepage": "https://github.com/quik-ui/quik-ui#readme",
  "keywords": [
    "react-native",
    "ui",
    "ui-kit",
    "components"
  ],
  "packageManager": "pnpm@8.15.2+sha256.90bb5d6382cb2cb8b8d4959a076b3953d84d1d94121717097bcd41c71344fa14",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.23.23",
    "@microsoft/api-extractor": "^7.40.1",
    "typescript": "5.3.3"
  }
